[run]
source = .
omit =
    tests/*
    test_*.py
    */__pycache__/*
    .pytest_cache/*
    htmlcov/*
    .git/*
    .github/*
    scripts/*
    *.pyc
    .coverage*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
